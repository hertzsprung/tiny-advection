FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

interpolationSchemes {
    default         none;
    interpolate(grad(Tf)) upwind phi;
}

divSchemes {
    default         none;
}

gradSchemes {
    default         none;
    default         Gauss upwind phi;
}

laplacianSchemes {
    default         Gauss linear corrected;
}

ddtSchemes {
    default         backward;
}

fluxRequired {
    default         no;
    T;
}
